@model IEnumerable<Practical_12.Models.CountEmpByDesination>

@{
    ViewBag.Title = "Query2";
}

<h2>Count Employee By Designation</h2>
<br />
<table class="table text-center table-hover">
    <tr class="table-dark">

        <th>
            @Html.DisplayNameFor(model => model.Designation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Count)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Designation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Count)
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Back to List", "Index", string.Empty, new { @class = "btn btn-success my-2 mx-2" })